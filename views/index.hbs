<h1>ЦИКЛО ДАВ ВАН ЛАВ</h1>
<p id="pressureRange"></p>
<p id="pressure"></p>
<p id="cycles"></p>
<p id="stage"></p>
<p id="pause"></p>
<button class="button" onclick="sendCommand('save')"> Сохранить настройки</button>
<button class="button" onclick="sendCommand('read')"> Считать настройки</button>
<div id="chartdiv"></div>
<button class="button" onclick="sendCommand('start')"> Старт</button>
<button class="button" onclick="sendCommand('stop')"> Стоп</button>
<button class="button" onclick="sendCommand('pause')"> Пауза</button>
<button class="button" onclick="sendCommand('up')"> Давление вверх</button>
<button class="button" onclick="sendCommand('down')"> Давление вниз</button>
<form method='GET' action='/settings' onsubmit=' sendForm(event); return false; '>
    <p>
        <label for='FtopPres'>Верхнее давление:</label>
        <input type='number' name='FtopPres' id='FtopPres'>
    </p>
    <p>
        <label for='FbotPres'>Нижнее давление:</label>
        <input type='number' name='FbotPres' id='FbotPres'></input>
    </p>
    <p>
        <label for='FcycleCount'>Количество циклов:</label>
        <input type='number' name='FcycleCount' id='FcycleCount'></input>
    </p>
    <p>
        <label for='topPause'>Верхняя пауза:</label>
        <input type='number' name='topPause' id='topPause'></input>
    </p>
    <p>
        <label for='botPause'>Нижняя пауза:</label>
        <input type='number' name='botPause' id='botPause'></input>
    </p>
    <p>
        <input type='submit' name='subm' value='Применить'>
        
    </p>
</form>
